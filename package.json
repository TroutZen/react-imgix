{
  "name": "react-native-imgix",
  "version": "0.0.2",
  "description": "React native port of React Imgix",
  "main": "lib/index.js",
  "standard": {
    "parser": "babel-eslint"
  },
  "scripts": {
    "build": "npm run build:lib",
    "prepublish": "npm run build",
    "build:lib": "babel src --out-dir lib",
    "test": "standard && mocha --require babel-core/register --recursive src/*.test.js",
    "test:watch": "mocha --require babel-core/register --recursive src/*.test.js --watch --reporter min"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imgix/react-native-imgix.git"
  },
  "keywords": [
    "react-native",
    "imgix"
  ],
  "author": "Devin Otway",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/imgix/react-native-imgix/issues"
  },
  "homepage": "https://github.com/imgix/react-native-imgix#readme",
  "peerDependencies": {
    "react": ">=0.14"
  },
  "dependencies": {
    "jsuri": "^1.3.1",
    "js-base64": "~2.1.9"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "^6.0.5",
    "babel-plugin-transform-object-assign": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "expect": "^1.12.2",
    "expect-jsx": "^2.1.0",
    "mocha": "^3.0.1",
    "mocha-babel": "^3.0.0",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0",
    "skin-deep": "^0.16.0",
    "standard": "^7.1.2",
    "tap": "^6.2.0"
  }
}
